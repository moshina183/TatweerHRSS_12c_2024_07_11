                        var="row" rows="#{bindings.ApprovalHistoryVO.rangeSize}"
                        emptyText="#{bindings.ApprovalHistoryVO.viewable ? 'No data to display.' : 'Access Denied.'}"
                        fetchSize="#{bindings.ApprovalHistoryVO.rangeSize}"
                        rowBandingInterval="0"
                        selectedRowKeys="#{bindings.ApprovalHistoryVO.collectionModel.selectedRow}"
                        selectionListener="#{bindings.ApprovalHistoryVO.collectionModel.makeCurrent}"
                        rowSelection="single" id="t5">
                <af:column sortProperty="#{bindings.ApprovalHistoryVO.hints.ApprovalType.name}"
                           sortable="false" width="150" align="center"
                           headerText="Type" id="c5">
                  <af:outputText value="#{row.ApprovalType}" id="ot1"/>
                </af:column>
                <af:column sortProperty="#{bindings.ApprovalHistoryVO.hints.AssigneeName.name}"
                           sortable="false" width="150" align="center"
                           headerText="Assignee Name" id="c8">
                  <af:outputText value="#{row.AssigneeName}" id="ot6"/>
                </af:column>
                <af:column sortProperty="#{bindings.ApprovalHistoryVO.hints.ActionTaken.name}"
                           sortable="false" width="150" align="center"
                           headerText="Status" id="c7">
                  <af:outputText value="#{row.ActionTaken}" id="ot5"
                                 inlineStyle="color:#{row.ActionTaken eq 'PENDING' ? 'Blue' : row.ActionTaken eq 'APPROVED' ? 'Green' : 'Red'};"/>
                </af:column>
                <af:column sortProperty="#{bindings.ApprovalHistoryVO.hints.SubmitttedOn.name}"
                           sortable="false" width="150" align="center"
                           headerText="Assigned On" id="c6">
                  <af:outputText value="#{row.SubmitttedOn}" id="ot3">
                    <af:convertDateTime pattern="dd/MM/yyyy hh:mm:ss a"/>
                  </af:outputText>
                </af:column>
                <af:column sortProperty="#{bindings.ApprovalHistoryVO.hints.ApprovalOn.name}"
                           sortable="false" width="150" align="center"
                           headerText="Action On" id="c4">
                  <af:outputText value="#{row.ApprovalOn}"
                                 inlineStyle="color:Blue;" id="ot4">
                    <af:convertDateTime pattern="dd/MM/yyyy hh:mm:ss a"/>
                  </af:outputText>
                </af:column>
              </af:table>
            </af:panelCollection>
