  <SQLQuery>
    <![CDATA[SELECT BusinessTripRequest.LOCAL_BUSINESS_TRIP_ID,
       BusinessTripRequest.PERSONE_ID,
       BusinessTripRequest.STEP_ID,
       BusinessTripRequest.START_DATE,
       BusinessTripRequest.END_DATE,
       BusinessTripRequest.DAYS,
       BusinessTripRequest.COUNTRY_ID,
       BusinessTripRequest.CITY_ID,
       BusinessTripRequest.DESTINATION_ID,
       BusinessTripRequest.LAUNDRY_ALLOWANCE_AMOUNT,
       BusinessTripRequest.TRANSPORTATION_ALLOWANCE_TYPE,
       BusinessTripRequest.TRANS_ALLOWANCE_AMOUNT,
       BusinessTripRequest.HOUSING_ALLOWANCE_TYPE,
       BusinessTripRequest.HOUSING_ALLOWANCE_AMOUNT,
       BusinessTripRequest.FOOD_ALLOWANCE_TYPE,
       BusinessTripRequest.FOOD_ALLOWANCE_AMOUNT,
       BusinessTripRequest.MEANS_TRAVEL,
       BusinessTripRequest.VISA_COST,
       BusinessTripRequest.OTHER_COST,
       BusinessTripRequest.BUDGET_FLAG,
       BusinessTripRequest.TRIP_TYPE,
       BusinessTripRequest.PERSONE_NAME,
       BusinessTripRequest.REQUEST_STATUS,
       BusinessTripRequest.REJECT_COMMENTS,
       BusinessTripRequest.ASSIGNEE,
       BusinessTripRequest.TRIP_REASON,
       BusinessTripRequest.EVENT_SUBJECT,
       BusinessTripRequest.EVENT_TYPE,
       BusinessTripRequest.REQUIRED_VISA,
       BusinessTripRequest.TARGETED_COMPETENCIES,
       BusinessTripRequest.COURSE_NAME,
       BusinessTripRequest.EXPENSE_BUSINESS_TRIP_ID,
       BusinessTripRequest.EXPENSE_FLAG,
       BusinessTripRequest.EXPENSE_TYPE,
       BusinessTripRequest.CANCEL_REASON,
       BusinessTripRequest.SUB_TYPE,
       BusinessTripRequest.NEW_CITY,
       BusinessTripRequest.DESTINATION_TYPE,
       BusinessTripRequest.CREATION_DATE,
       BusinessTripRequest.COST_CENTER,
       BusinessTripRequest.COST_CENTER_NUMBER,
       BusinessTripRequest.TRAINING_COST,
       BusinessTripRequest.OTHER_MEANS_TRAVEL,
       BusinessTripRequest.TRAVEL_COST,
       BusinessTripRequest.EVENT_COST,
       BusinessTripRequest.TRIP_COMMENT,
       BusinessTripRequest.PERSON_POSITION,
       BusinessTripRequest.PERSON_JOB,
       BusinessTripRequest.PERSON_DEPARTMENT,
       BusinessTripRequest.PERSON_LOCATION,
       BusinessTripRequest.OTHER_CITIES,
       BusinessTripRequest.TO_CITY,
       BusinessTripRequest.ACTION_TAKEN,
       BusinessTripRequest.TOTAL_FOR_INVOICE,
 BusinessTripRequest.PER_DIEM_AMOUNT,
  DECODE(BusinessTripRequest.BENEFIT_TYPE,'A',
  nvl(BusinessTripRequest.LAUNDRY_ALLOWANCE_AMOUNT,0)+nvl(BusinessTripRequest.TRANS_ALLOWANCE_AMOUNT,0)+nvl(BusinessTripRequest.HOUSING_ALLOWANCE_AMOUNT,0)+nvl(BusinessTripRequest.FOOD_ALLOWANCE_AMOUNT,0)+nvl(BusinessTripRequest.VISA_COST,0)+nvl(BusinessTripRequest.OTHER_COST,0)+nvl(BusinessTripRequest.TRAINING_COST,0)+nvl(BusinessTripRequest.TRAVEL_COST,0)+nvl(BusinessTripRequest.EVENT_COST,0)
  ,nvl(BusinessTripRequest.PER_DIEM_AMOUNT,0)+nvl(BusinessTripRequest.VISA_COST,0)+nvl(BusinessTripRequest.OTHER_COST,0)+nvl(BusinessTripRequest.TRAINING_COST,0)+nvl(BusinessTripRequest.TRAVEL_COST,0)+nvl(BusinessTripRequest.EVENT_COST,0)) as total_amount,
 DECODE(BusinessTripRequest.BENEFIT_TYPE,'A','Allowances','Per-Diem') AS BENEFIT_TYPE
FROM BUSINESS_TRIP_REQUEST  BusinessTripRequest]]>
  </SQLQuery>
