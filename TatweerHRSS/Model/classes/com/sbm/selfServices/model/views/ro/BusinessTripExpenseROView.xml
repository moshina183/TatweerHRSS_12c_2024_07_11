    businesstriprequest.TRAVEL_COST,
    businesstriprequest.EVENT_COST,
    businesstriprequest.TRIP_COMMENT,
    businesstriprequest.PERSON_POSITION,
    businesstriprequest.PERSON_JOB,
    businesstriprequest.PERSON_DEPARTMENT,
    businesstriprequest.PERSON_LOCATION,
    businesstriprequest.OTHER_CITIES,
    businesstriprequest.TO_CITY,
    businesstriprequest.ACTION_TAKEN,
    businesstriprequest.PER_DIEM_AMOUNT,
    businesstriprequest.BENEFIT_TYPE,
    businesstriprequest.TOTAL_FOR_INVOICE,
    businesstriprequest.division,
    businesstriprequest.lob,
businesstriprequest.actual_start_date,
businesstriprequest.actual_end_date,
businesstriprequest.ONE_WAY_TRIP,
businesstriprequest.more_cities_check


FROM
    business_trip_request businesstriprequest
WHERE
        businesstriprequest.persone_id =:bind_persone_id
    AND
        businesstriprequest.request_status = 'APPROVED'
    AND
        businesstriprequest.expense_flag = 'N'
    AND 
        businesstriprequest.trip_type !='Expense'     
