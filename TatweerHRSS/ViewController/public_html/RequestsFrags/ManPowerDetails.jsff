<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:pageTemplate viewId="/oracle/apps/uikit/common/pageTemplates/MainPageTemplate.jspx"
                   id="pt1">
    <f:facet name="pageContents">
      <af:panelBox id="pb1">
        <af:panelBox text="Manpower Requisition Request Details - #{bindings.RequestStatus.inputValue}" id="pb2"
                     inlineStyle="background-color:#ffffff;">
          <f:facet name="toolbar">
            <af:toolbar id="t1">
              <af:commandToolbarButton text="Save" id="ctb1" immediate="true"
                                       action="#{pageFlowScope.ManpowerMB.saveRequest}"
                                       rendered="#{bindings.StepId.inputValue eq 1 and !(bindings.RequestStatus.inputValue eq 'APPROVED' || bindings.RequestStatus.inputValue eq 'REJECTED')}"/>
              <af:commandToolbarButton text="Submit" id="ctb2" immediate="true"
                                       action="#{pageFlowScope.ManpowerMB.submitRequest}"
                                       rendered="#{bindings.StepId.inputValue eq 1 || bindings.RequestStatus.inputValue eq 'SAVED'}"/>
              <af:commandToolbarButton text="Approve" id="ctb3" immediate="true"
                                       action="#{pageFlowScope.ManpowerMB.approveRequest}"
                                       rendered="#{PersonInfo.fullName eq bindings.AssigneeName.inputValue and bindings.RequestStatus.inputValue eq 'PENDING'}"/>
              <af:commandToolbarButton text="Reject" id="ctb4"
                                       rendered="#{PersonInfo.fullName eq bindings.AssigneeName.inputValue and bindings.RequestStatus.inputValue eq 'PENDING'}">
                <af:showPopupBehavior popupId="p1" triggerType="action"
                                      align="afterEnd"/>
              </af:commandToolbarButton>
              <af:commandToolbarButton text="Approve Withdraw" id="ctb7"
                                   rendered="#{PersonInfo.fullName eq bindings.AssigneeName.inputValue and bindings.RequestStatus.inputValue eq 'Waiting Withdraw Approval'}"
                                   action="#{pageFlowScope.ManpowerMB.approve_withdraw}"/>
            <af:commandToolbarButton text="Reject Withdraw" id="ctb8"
                                   rendered="#{PersonInfo.fullName eq bindings.AssigneeName.inputValue and bindings.RequestStatus.inputValue eq 'Waiting Withdraw Approval'}"
                                   action="#{pageFlowScope.ManpowerMB.reject_withdraw}"/>
              <af:popup id="p1">
                <af:dialog id="d1" type="cancel"
                           title="Please Enter Rejection Comment">
                  <f:facet name="buttonBar">
                    <af:commandButton text="Reject" id="cb1"
                                      action="#{pageFlowScope.ManpowerMB.rejectRequest}"/>
                  </f:facet>
                  <af:inputText value="#{bindings.RejectionReason.inputValue}"
                                required="#{bindings.RejectionReason.hints.mandatory}"
                                columns="50"
                                maximumLength="#{bindings.RejectionReason.hints.precision}"
                                shortDesc="#{bindings.RejectionReason.hints.tooltip}"
                                id="it9" rows="5">
                    <f:validator binding="#{bindings.RejectionReason.validator}"/>
                  </af:inputText>
                </af:dialog>
              </af:popup>
              <af:commandToolbarButton text="Cancel" id="ctb5"
                                       actionListener="#{bindings.Rollback.execute}"
                                       action="backToManPowerReq"
                                       immediate="true">
                    <af:resetActionListener/>                       
                </af:commandToolbarButton>
            </af:toolbar>
          </f:facet>
          <af:spacer width="10" height="20" id="s2"/>
