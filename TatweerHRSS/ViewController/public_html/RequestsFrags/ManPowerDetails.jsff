                      <af:commandToolbarButton text="Cancel" id="ctb10"
                                               action="#{pageFlowScope.ManpowerMB.cancelBtnAction}"
                                               immediate="true"/>
                    </af:toolbar>
                  </af:panelFormLayout>
                </af:dialog>
              </af:popup>
            </af:panelBox>
          </af:panelGroupLayout>
          <af:spacer width="10" height="10" id="s10"/>
