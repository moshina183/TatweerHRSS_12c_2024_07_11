                          fetchSize="#{bindings.ManpowerAttachmentsEOView1.rangeSize}"
                          rowBandingInterval="0"
                          selectedRowKeys="#{bindings.ManpowerAttachmentsEOView1.collectionModel.selectedRow}"
                          selectionListener="#{bindings.ManpowerAttachmentsEOView1.collectionModel.makeCurrent}"
                          rowSelection="single" id="t2"
                          partialTriggers="cl2 :::ctb6">
                  <af:column sortProperty="#{bindings.ManpowerAttachmentsEOView1.hints.FileName.name}"
                             sortable="false"
                             headerText="#{bindings.ManpowerAttachmentsEOView1.hints.FileName.label}"
                             id="c2">
                    <!--<af:outputText value="#{row.FileName}" id="ot1"/>-->
                    <af:commandLink text="#{row.FileName}" id="cl1">
                      <af:fileDownloadActionListener filename="#{row.FileName}"
                                                     contentType="#{row.FileType}"
                                                     method="#{pageFlowScope.ManpowerMB.downloadFile}"/>
                    </af:commandLink>
                  </af:column>
                  <af:column sortProperty="#{bindings.ManpowerAttachmentsEOView1.hints.UploadedBy.name}"
