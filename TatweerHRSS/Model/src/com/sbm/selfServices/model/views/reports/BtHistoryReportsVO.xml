  <SQLQuery>
    <![CDATA[SELECT BtHistory.BT_HISTORY_ID,
       BtHistory.LOCAL_BUSINESS_TRIP_ID,
       BtHistory.PERSONE_ID,
       BtHistory.STEP_ID,
       BtHistory.START_DATE,
       BtHistory.END_DATE,
       BtHistory.DAYS,
       BtHistory.COUNTRY_ID,
       BtHistory.CITY_ID,
       BtHistory.DESTINATION_ID,
       BtHistory.LAUNDRY_ALLOWANCE_AMOUNT,
       BtHistory.TRANSPORTATION_ALLOWANCE_TYPE,
       BtHistory.TRANS_ALLOWANCE_AMOUNT,
       BtHistory.HOUSING_ALLOWANCE_TYPE,
       BtHistory.HOUSING_ALLOWANCE_AMOUNT,
       BtHistory.FOOD_ALLOWANCE_TYPE,
       BtHistory.FOOD_ALLOWANCE_AMOUNT,
       BtHistory.MEANS_TRAVEL,
       BtHistory.VISA_COST,
       BtHistory.OTHER_COST,
       BtHistory.BUDGET_FLAG,
       BtHistory.TRIP_TYPE,
       BtHistory.PERSONE_NAME,
       BtHistory.REQUEST_STATUS,
       BtHistory.REJECT_COMMENTS,
       BtHistory.ASSIGNEE,
       BtHistory.TRIP_REASON,
       BtHistory.EVENT_SUBJECT,
       BtHistory.EVENT_TYPE,
       BtHistory.REQUIRED_VISA,
       BtHistory.TARGETED_COMPETENCIES,
       BtHistory.COURSE_NAME,
       BtHistory.EXPENSE_BUSINESS_TRIP_ID,
       BtHistory.EXPENSE_FLAG,
       BtHistory.EXPENSE_TYPE,
       BtHistory.CANCEL_REASON,
       BtHistory.SUB_TYPE,
       BtHistory.NEW_CITY,
       BtHistory.DESTINATION_TYPE,
       BtHistory.CREATION_DATE,
       BtHistory.COST_CENTER,
       BtHistory.COST_CENTER_NUMBER,
       BtHistory.TRAINING_COST,
       BtHistory.OTHER_MEANS_TRAVEL,
       BtHistory.TRAVEL_COST,
       BtHistory.EVENT_COST,
       BtHistory.TRIP_COMMENT,
       BtHistory.OTHER_CITIES,
       BtHistory.TO_CITY,
       BtHistory.ACTION_TAKEN,
BtHistory.TOTAL_FOR_INVOICE,
 BtHistory.PER_DIEM_AMOUNT,
  DECODE(BtHistory.BENEFIT_TYPE,'A',
  nvl(BtHistory.LAUNDRY_ALLOWANCE_AMOUNT,0)+nvl(BtHistory.TRANS_ALLOWANCE_AMOUNT,0)+nvl(BtHistory.HOUSING_ALLOWANCE_AMOUNT,0)+nvl(BtHistory.FOOD_ALLOWANCE_AMOUNT,0)+nvl(BtHistory.VISA_COST,0)+nvl(BtHistory.OTHER_COST,0)+nvl(BtHistory.TRAINING_COST,0)+nvl(BtHistory.TRAVEL_COST,0)+nvl(BtHistory.EVENT_COST,0)
  ,nvl(BtHistory.PER_DIEM_AMOUNT,0)+nvl(BtHistory.VISA_COST,0)+nvl(BtHistory.OTHER_COST,0)+nvl(BtHistory.TRAINING_COST,0)+nvl(BtHistory.TRAVEL_COST,0)+nvl(BtHistory.EVENT_COST,0)) as total_amount,
 DECODE(BtHistory.BENEFIT_TYPE,'A','Allowances','Per-Diem') AS BENEFIT_TYPE
FROM BT_HISTORY BtHistory]]>
  </SQLQuery>
